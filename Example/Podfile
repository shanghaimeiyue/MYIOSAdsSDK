use_frameworks!

platform :ios, '12.0'

target 'MYIOSAdsSDK_Example' do
  pod 'MYIOSAdsSDK', :path => '../'
  
  pod 'Ads-CN-Beta', '~> 6.8.0.5', :subspecs => ['BUAdSDK', 'CSJMediation']
  pod 'GDTMobSDK', '~> 4.15.30'
  pod 'KSAdSDK', '~> 3.3.76.5'
#  pod 'QMAdSDK', '~> 1.3.3'
#  pod 'UBiXMediationSDK', '2.8.1'
#  pod 'UBiXMUBiXAdapter'
#  pod 'JADYun', '2.6.8'
#  pod 'BaiduMobAdSDK', '5.39.0'

  pod 'WechatOpenSDK_UnPay'

  target 'MYIOSAdsSDK_Tests' do
    inherit! :search_paths

    
  end
end

post_install do |installer|
  installer.pods_project.targets.each do |target|
    if ['Masonry', 'MBProgressHUD'].include?(target.name)
      target.build_configurations.each do |config|
        config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '11.0'
      end
    end
  end
end
